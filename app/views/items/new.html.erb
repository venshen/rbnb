<%= simple_form_for @items do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :deposit %>
  <%= f.number_field :deposit %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :picture_url %>
  <%= f.text_field :picture_url %>
  <%= f.submit %>
<% end %>
